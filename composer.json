{
    "name": "tadasei/laravel-contacts",
    "description": "A package that provides stubs for managing contact info associations with models",
	"keywords": ["laravel", "contact", "stubs", "backend"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tadasei",
            "email": "chemselhak.mahouche@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2.0",
        "illuminate/console": "^12.0",
        "illuminate/filesystem": "^12.0",
        "illuminate/support": "^12.0",
        "illuminate/validation": "^12.0",
        "nnjeim/world": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpstan/phpstan": "^2.1",
        "symfony/var-dumper": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Tadasei\\Contacts\\": "src/"
        }
    },
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"Tadasei\\Contacts\\ServiceProvider"
			]
		}
	}
}
